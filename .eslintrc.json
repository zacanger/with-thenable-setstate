{
  "root": true,
  "plugins": [
    "zacanger"
  ],
  "extends": [
    "plugin:zacanger/recommended",
    "plugin:zacanger/react",
    "plugin:zacanger/prettier"
  ],
  "rules": {
    "fp/no-nil": 0,
    "fp/no-class": 0,
    "fp/no-unused-expression": 0,
    "fp/no-this": 0,
    "fp/no-mutation": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
